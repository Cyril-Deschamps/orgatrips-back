{
  "name": "orgatrips",
  "version": "0.0.0",
  "description": "OrgaTrips Node JS BackEnd - Rest API",
  "main": "scripts/server.js",
  "private": true,
  "dependencies": {
    "axios": "^0.21.4",
    "axios-retry": "^3.4.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.14.1",
    "chalk": "^5.0.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.0.0",
    "compression": "^1.6.0",
    "content-disposition": "^0.5.3",
    "cookies": "^0.8.0",
    "core-js": "^3.16.0",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.13.3",
    "inquirer": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^3.0.1",
    "mime": "^2.5.2",
    "morgan": "^1.6.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.2.0",
    "node-mailjet": "^3.3.4",
    "node-schedule": "^2.0.0",
    "path": "^0.12.7",
    "sequelize": "^6.6.5",
    "uid-safe": "^2.1.1",
    "winston": "^3.2.1",
    "yargs": "^17.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "nodemon --experimental-specifier-resolution=node scripts/server.js",
    "start": "node --experimental-specifier-resolution=node scripts/server.js",
    "user": "node --experimental-specifier-resolution=node scripts/user.js",
    "cities": "node --experimental-specifier-resolution=node scripts/cities.js",
    "doc": "apidoc -i app/ -o apidoc/",
    "sync": "node --experimental-specifier-resolution=node scripts/sync.js",
    "lint-check:node": "cross-env NODE_ENV=production && eslint *.cjs app scripts --ext .js,.cjs,.mjs --no-fix --max-warnings=0",
    "lint-check:prettier": "prettier --check *.cjs app scripts",
    "lint-fix:node": "cross-env NODE_ENV=production && eslint *.cjs app scripts --ext .js,.cjs,.mjs --fix --max-warnings=0",
    "lint-fix:prettier": "prettier --write *.cjs app scripts"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "apidoc": "^0.28.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "semantic-release": "^18.0.1"
  },
  "type": "module",
  "nodemonConfig": {
    "watch": [
      ".env",
      "scripts/server.js",
      "app"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
